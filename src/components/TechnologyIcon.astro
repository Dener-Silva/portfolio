---
import { assert } from "./Assert";
const { name, title } = Astro.props;
const src = `${import.meta.env.BASE_URL}${name
    ?.replace(/\W/g, "-")
    .toLowerCase()}.svg`;
assert(name, "TechnologyIcon must have a name");
---

<img
    src={src}
    style="filter: brightness(0); object-fit: contain;"
    height="23px"
    width="23px"
    alt={title || ""}
    title={title || ""}
/>
